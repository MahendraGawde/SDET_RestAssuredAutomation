{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "name": {
        "type": "string"
      },
      "location": {
        "type": "string"
      },
      "phone": {
        "type": "string"
      },
      "courses": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "empty": {
        "type": "boolean"
      }
    },
    "required": ["id"],
    "additionalProperties": false
  }
}
